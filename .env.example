# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Reown AppKit Configuration
NEXT_PUBLIC_REOWN_PROJECT_ID=your_reown_project_id

# Solana RPC Configuration (Helius recommended for production)
NEXT_PUBLIC_HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_helius_api_key

# Vultr Configuration (for OpenClaw VM provisioning)
VULTR_API_KEY=your_vultr_api_key

# Encryption Key (32-byte hex for encrypting sensitive tokens)
ENCRYPTION_KEY=your_32_byte_hex_key

# ClawBrick Domain Configuration
CLAWBRICK_DOMAIN=clawbrick.com
CONTROL_SERVER_IP=your_control_server_ip

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=ClawBrick

